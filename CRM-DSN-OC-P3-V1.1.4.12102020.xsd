<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="qualified">
	<xs:element name="Fichier">
		<xs:annotation>
			<xs:documentation>Données descriptives du fichier reçu</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="2">
				<xs:element name="Declaration">
					<xs:annotation>
						<xs:documentation>Données descriptives d'une déclaration DSN</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence minOccurs="0" maxOccurs="1">
							<xs:choice minOccurs="0">
								<xs:element name="MandatNetE" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Mandat à gérer sur Net-Entreprises</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="xs:string">
												<xs:attribute name="BIC" use="required">
													<xs:annotation>
														<xs:documentation>BIC du compte débiteur</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="[A-Z]{6}[A-Z0-9]{2}"/>
															<xs:pattern value="[A-Z]{6}[A-Z0-9]{5}"/>
															<xs:pattern value="[_]{11}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="IBAN" use="required">
													<xs:annotation>
														<xs:documentation>IBAN du compte débiteur</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="15"/>
															<xs:maxLength value="34"/>
															<xs:pattern value="[A-Z]{2}[0-9]{2}[A-Z0-9]+"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ICS" use="required">
													<xs:annotation>
														<xs:documentation>ICS du créancier</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="[F][R][A-Z0-9]{5}[0-9]{6}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="MandatProp" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Mandat à gérer par une solution propriétaire</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="xs:string">
												<xs:attribute name="BIC" use="required">
													<xs:annotation>
														<xs:documentation>BIC du compte débiteur</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="[A-Z]{6}[A-Z0-9]{2}"/>
															<xs:pattern value="[A-Z]{6}[A-Z0-9]{5}"/>
															<xs:pattern value="[_]{11}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="IBAN" use="required">
													<xs:annotation>
														<xs:documentation>IBAN du compte débiteur</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="15"/>
															<xs:maxLength value="34"/>
															<xs:pattern value="[A-Z]{2}[0-9]{2}[A-Z0-9]+"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ICS" use="required">
													<xs:annotation>
														<xs:documentation>ICS du créancier</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="[F][R][A-Z0-9]{5}[0-9]{6}"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="URLMandatProp">
													<xs:annotation>
														<xs:documentation>URL du service Mandat 
propriétaire de l'OC</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="10"/>
															<xs:maxLength value="250"/>
															<xs:pattern value="(http)(s)?(://)[&#x20;-&#x7E;]+"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:choice>
							<xs:element name="RetourMetier" minOccurs="0" maxOccurs="2000">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ContexteRetourMetier" minOccurs="0" maxOccurs="15">
