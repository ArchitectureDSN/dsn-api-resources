<?xml version="1.0" encoding="ISO-8859-1"?>
<!--Schéma XSD du CRM (compte-rendu du traitement OC d'une DSN mensuelle ou signalement fin de contrat) V8.31-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="qualified">
	<xs:element name="Fichier">
		<xs:annotation>
			<xs:documentation>Données descriptives du fichier reçu</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="1">
				<xs:element name="Declaration">
					<xs:annotation>
						<xs:documentation>Données descriptives d'une déclaration DSN</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence minOccurs="0" maxOccurs="1">
							<xs:element name="Diagnostic" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Données de diagnostic du traitement de la déclaration et des anomalies rencontrées</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="CodeRetourDetail" use="required">
										<xs:annotation>
											<xs:documentation>000 = DSN entièrement acceptée
010 = Entreprise inconnue de l'organisme récepteur
019 = Autre anomalie concernant l'entreprise ou l'établissement déclaré
020 = Anomalie concernant les données salarié(s) déclarées
030 = Contrat(s) inconnu(s)
031 = Autre(s) anomalie(s) sur les références contrat déclarées
040 = Code(s) option déclaré(s)  absent(s) ou déclaré(s) mais inconnu(s)
049 = Autre(s) anomalie(s) sur code(s) option
050 = Code(s)  population déclaré(s)  absent(s) ou déclaré(s) mais  ou inconnu(s)
059 = Autre(s) anomalie(s) sur code(s)  population
060 = Code délégataire de gestion incorrect
061 = Code organisme de Prévoyance incorrect
100 = Evènement(s)  déjà déclaré(s)
101 = Evènement(s)  rejeté(s), voir descriptif associé
109 = Autre(s) anomalie(s) concernant le ou les événements reçus  
800 = Anomalie grave de structure ou de contenu
801 = Déclaration en double ou "annule et remplace" inexploitable
999 = Détail fourni aux niveaux 3 et 4</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="000"/>
												<xs:enumeration value="010"/>
												<xs:enumeration value="019"/>
												<xs:enumeration value="020"/>
												<xs:enumeration value="030"/>
												<xs:enumeration value="031"/>
												<xs:enumeration value="040"/>
												<xs:enumeration value="049"/>
												<xs:enumeration value="050"/>
												<xs:enumeration value="059"/>
												<xs:enumeration value="060"/>
												<xs:enumeration value="061"/>
												<xs:enumeration value="100"/>
												<xs:enumeration value="101"/>
												<xs:enumeration value="109"/>
												<xs:enumeration value="800"/>
												<xs:enumeration value="801"/>
												<xs:enumeration value="999"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ComplementRetourDetail">
										<xs:annotation>
											<xs:documentation>Complément explicatif libre du code retour détail</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="3"/>
												<xs:maxLength value="250"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Salarie" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Données relatives à un salarié</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence minOccurs="1" maxOccurs="1">
										<xs:element name="Evt" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Données relatiives à un événement détecté (explicite ou implicite.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="CodeEvt" use="required">
													<xs:annotation>
														<xs:documentation>Code événement
01 = affiliation
02 = changement d'option
04 = changement de population
10 = radiation
11 = entrée en maintien de garanties (ANI)
81 = changement de données administratives du salarié (adresse ...)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="01"/>
															<xs:enumeration value="02"/>
															<xs:enumeration value="04"/>
															<xs:enumeration value="10"/>
															<xs:enumeration value="11"/>
															<xs:enumeration value="81"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="DateEvt" use="required">
													<xs:annotation>
														<xs:documentation>Date événement prise en compte (explicite ou implicite)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:length value="8"/>
															<xs:pattern value="(0[1-9]|[1-2][0-9]|3[0-1])(0[1-9]|1[0-2])(19|20)\d\d"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="SrcEvt" use="required">
													<xs:annotation>
														<xs:documentation>Source de l'événement :
01 = fin de contrat en S21.G00.62 dans un signalement
02 = fin de contrat en S21.G00.62 dans une mensuelle
03 = changement salarié S21.G00.31 dans une mensuelle
04 = changement contrat S21.G00.41 dans une mensuelle
05 = début de contrat de travail détecté dans une mensuelle
06 = changement d'option détecté dans une mensuelle, via S21.G00.70.004
07 = changement de population détecté dans une mensuelle, via S21.G00.70.005
08 = changement de population détecté dans une mensuelle, via S21.G00.40</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="01"/>
															<xs:enumeration value="02"/>
															<xs:enumeration value="03"/>
															<xs:enumeration value="04"/>
															<xs:enumeration value="05"/>
															<xs:enumeration value="06"/>
															<xs:enumeration value="07"/>
															<xs:enumeration value="08"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="CodeRetourEvt" use="required">
													<xs:annotation>
														<xs:documentation>Code retour pour l'événement et le salarié concernés
00 = événement pris en compte
01 = événement ignoré - déjà déclaré
10 = événement rejeté suite à rejet sur le salarié (voir code retour salarié-contrat)
20 = événement rejeté - changement d'option incohérent
30 = événement rejeté - changement de population incohérent
99 = événement rejeté - autre cas</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="00"/>
															<xs:enumeration value="01"/>
															<xs:enumeration value="10"/>
															<xs:enumeration value="20"/>
															<xs:enumeration value="30"/>
															<xs:enumeration value="99"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ComplementRetourEvt" use="optional">
													<xs:annotation>
														<xs:documentation>Complément explicatif au code retour événement / salarié - obligatoire si code retour différent de 00</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="3" fixed="false"/>
															<xs:maxLength value="250" fixed="false"/>
															<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="NirSal" use="optional">
										<xs:annotation>
											<xs:documentation>NIR du salarié - S21.G00.30.001
A compter de la phase 2, s'il n'y a pas de NIR pour le salarié, alors le NTT doit être renseigné</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="13"/>
												<xs:pattern value="[1-2][0-9]{2}(0[1-9]|1[0-2]|20|3[0-9]|4[0-2]|[5-9][0-9])(0[1-9]|[1-9][0-9]|2A|2B)([0]{2}[1-9]|0[1-9][0-9]|[1-9][0-9]{2})([0]{2}[1-9]|0[1-9][0-9]|[1-9][0-9]{2})|([1-2][9]{12})"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="NTTSal" use="optional">
										<xs:annotation>
											<xs:documentation>Uniquement à compter de la phase 2 
NTT salarié - S21.G00.30.020</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="11"/>
												<xs:maxLength value="40"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="NomSal" use="required">
										<xs:annotation>
											<xs:documentation>Nom du salarié - S21.G00.30.002</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="80"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="PrenomSal" use="required">
										<xs:annotation>
											<xs:documentation>Prénom du salarié - S21.G00.30.004</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="80"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="NomUsageSal">
										<xs:annotation>
											<xs:documentation>Nom d'usage du salarié - S21.G00.30.003</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="80"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="MatriculeSal" use="optional">
										<xs:annotation>
											<xs:documentation>Matricule du salarié - S21.G00.80.003
(remplacé par S21.G00.30.019 en DSN phase 2)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="RefContrat">
										<xs:annotation>
											<xs:documentation>Référence du contrat déclarée - S21.G00.70.001</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="RefContratAtt">
										<xs:annotation>
											<xs:documentation>Référence du contrat attendue (donnée éventuellement retournée par le producteur du CRM pour correction)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="RefContratAj">
										<xs:annotation>
											<xs:documentation>Référence du contrat ajoutée (donnée éventuellement retournée par le producteur du CRM dans le cas d'un contrat manquant)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="CodeOption" use="optional">
										<xs:annotation>
											<xs:documentation>Code option déclaré - S21.G00.70.004</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="CodeOptionAtt" use="optional">
										<xs:annotation>
											<xs:documentation>Code option attendu (donnée éventuellement retournée par le producteur du CRM pour correction)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="CodeOptionAj" use="optional">
										<xs:annotation>
											<xs:documentation>Code option ajouté (donnée éventuellement retournée par le producteur du CRM dans le cas d'un contrat manquant)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="CodePopulation" use="optional">
										<xs:annotation>
											<xs:documentation>Code population déclaré - S21.G00.70.005</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="CodePopulationAtt" use="optional">
										<xs:annotation>
											<xs:documentation>Code population attendu (donnée éventuellement retournée par le producteur du CRM pour correction)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="CodePopulationAj" use="optional">
										<xs:annotation>
											<xs:documentation>Code population ajouté (donnée éventuellement retournée par le producteur du CRM dans le cas d'un contrat manquant)</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="30"/>
												<xs:pattern value="[&#x20;&#x22;&#x26;-&#x3A;&#x3D;&#x40;-&#x5A;&#x5F;-&#x7A;&#xAB;&#xB0;&#xBB;&#xC0;-&#xC5;&#xC7;-&#xCF;&#xD1;-&#xD6;&#xD9;-&#xDD;&#xE0;-&#xE5;&#xE7;-&#xEF;&#xF1;-&#xF6;&#xF9;-&#xFD;&#xFF;]+"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="CodeRetourSal" use="required">
										<xs:annotation>
											<xs:documentation>Code retour de niveau salarié-contrat
00 = OK
10 = contrat inconnu
11 = contrat invalide pour ce salarié
19 = autre anomalie relative au contrat
20 = code option absent mais requis pour ce salarié et/ou contrat
21 = code option invalide pour ce contrat
22 = code option invalide pour ce salarié
23 = code option invalide pour ce code population
29 = autre anomalie relative au code option
30 = code population absent mais requis pour ce salarié et/ou contrat
31 = code population invalide pour ce contrat
32 = code population invalide pour ce salarié
39 = autre anomalie relative au code population
99 = autres cas d'erreur liés au salarié-contrat</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="00"/>
												<xs:enumeration value="10"/>
												<xs:enumeration value="11"/>
												<xs:enumeration value="19"/>
												<xs:enumeration value="20"/>
												<xs:enumeration value="21"/>
												<xs:enumeration value="22"/>
												<xs:enumeration value="23"/>
												<xs:enumeration value="29"/>
												<xs:enumeration value="30"/>
												<xs:enumeration value="31"/>
												<xs:enumeration value="32"/>
												<xs:enumeration value="39"/>
												<xs:enumeration value="99"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="ComplementRetourSal" use="optional">
										<xs:annotation>
											<xs:documentation>Complément explicatif au code retour salarié-contrat - obligatoire si code retour différent de 00</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="3"/>
												<xs:maxLength value="250"/>
